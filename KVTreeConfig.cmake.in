set(KVTREE_VERSION 0.0.0)

@PACKAGE_INIT@

set_and_check(KVTREE_INCLUDE_DIRS "@PACKAGE_CMAKE_INSTALL_INCLUDEDIR@")
set_and_check(KVTREE_LIBRARIES "@PACKAGE_CMAKE_INSTALL_LIBDIR@/libkvtree.a")

FIND_PACKAGE(ZLIB REQUIRED)
IF(ZLIB_FOUND)
    LIST(APPEND KVTREE_INCLUDE_DIRS ${ZLIB_INCLUDE_DIRS})
    LIST(APPEND KVTREE_LIBRARIES ${ZLIB_LIBRARIES})
ENDIF(ZLIB_FOUND)

check_required_components(KVTREE)
